# Copy this file to .env and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# App Environment (local | development | production | self-hosted)
VITE_ENVIRONMENT=development

# App Environment for feature flags (dev | prod)
VITE_APP_ENVIRONMENT=dev

# API URL
VITE_API_URL=http://localhost:8080/v1

# PostHog Analytics
VITE_APP_PUBLIC_POSTHOG_KEY=your-posthog-key-here
VITE_APP_PUBLIC_POSTHOG_HOST=https://app.posthog.com

# Sentry Error Tracking (Optional)
VITE_APP_PUBLIC_SENTRY_DSN=your-sentry-dsn-here

# Intercom Support Chat (Optional)
VITE_APP_INTERCOM_APP_ID=your-intercom-app-id-here
